# DevHand Local Configuration (gitignored, contains secrets)
# Copy this to .dh.local.toml and fill in your credentials

[db]
# Database URL (Supabase project URL)
url = "https://your-project-ref.supabase.co"

# Service role key (from Supabase Dashboard > Settings > API)
service_role_key = "your-service-role-key"

# Anonymous key (from Supabase Dashboard > Settings > API)
anon_key = "your-anon-key"

# Database password (from Supabase Dashboard > Settings > Database)
password = "your-database-password"

# Project reference (extracted from URL automatically, but can be overridden)
# project_ref = "your-project-ref"
